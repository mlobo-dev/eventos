 } from 'react';\nimport './usuario-novo.css';\nimport firebase from '../../config/firebase';\nimport 'firebase/auth';\n\nimport Navbar from '../../components/navbar';\nimport { Redirect } from 'react-router-dom';\n\nfunction NovoUsuario() {\n  const [email, setEmail] = useState();\n  const [senha, setSenha] = useState();\n  const [msgTipo, setMsgTipo] = useState();\n  const [msg, setMsg] = useState();\n  const [carregando, setCarregando] = useState(false);\n\n  function cadastrar() {\n    setCarregando(true);\n\n    setMsgTipo(null);\n    if (!email || !senha) {\n      setMsgTipo('erro');\n      setMsg('Você precisa informar o email e senha para fazer o cadastro!');\n      setCarregando(false);\n      return;\n    }\n\n    firebase\n      .auth()\n      .createUserWithEmailAndPassword(email, senha)\n      .then((response) => {\n        setMsgTipo('sucesso');\n        setCarregando(false);\n      })\n      .catch((erro) => {\n        setMsgTipo('erro');\n        console.table(erro);\n        setMsg(erro.message);\n        setCarregando(false);\n      });\n  }\n\n  return (\n    <>\n      <Navbar />\n      <div className=\"form-cadastro\">\n        <form className=\"text-center form-login mx-auto mt-5\">\n          <h1 className=\"h3 mb-3 text-black font-weigth-bold\">Cadastro</h1>\n\n          <input\n            type=\"email\"\n            className=\"form-control my-2\"\n            placeholder=\"Email\"\n            required\n            onChange={(e) => setEmail(e.target.value)}\n          />\n\n          <input\n            type=\"password\"\n            className=\"form-control my-2\"\n            placeholder=\"Email\"\n            required\n            onChange={(e) => setSenha(e.target.value)}\n          />\n\n          {carregando ? (\n            <div class=\"spinner-border text-danger \" role=\"status\">\n              <span class=\"sr-only\">Loading...</span>\n            </div>\n          ) : (\n            <button\n              type=\"button\"\n              className=\"btn  btn-lg  btn-block  mt-3  mb-5  btn-cadastrar\"\n              onClick={cadastrar}\n            >\n              Cadastrar\n            </button>\n          )}\n\n          <div className=\"msg-login text-black text-center my-5\">\n            {msgTipo === 'sucesso' && (\n              <span>\n                <strong>Wow!</strong> Usuário cadastrado com sucesso! &#128526;\n              </span>\n            )}\n\n            {msgTipo === 'erro' && (\n              <span>\n                <strong>Ops! </strong>\n                {msg} &#128546;\n              </span>\n            )}\n          </div>\n        </form>\n      </div>\n    </>\n  );\n}\n\nexport default NovoUsuario;\n","/home/mlobo/Desktop/Workspaces/VS Code/WolfLabs/eventos/src/routes/router.js",[],"/home/mlobo/Desktop/Workspaces/VS Code/WolfLabs/eventos/src/view/home/index.js",[],"/home/mlobo/Desktop/Workspaces/VS Code/WolfLabs/eventos/src/components/navbar/index.js",[],"/home/mlobo/Desktop/Workspaces/VS Code/WolfLabs/eventos/src/store/index.js",[],"/home/mlobo/Desktop/Workspaces/VS Code/WolfLabs/eventos/src/store/usuarioReducer.js",[],"/home/mlobo/Desktop/Workspaces/VS Code/WolfLabs/eventos/src/view/recuperar-senha/index.js",[],"/home/mlobo/Desktop/Workspaces/VS Code/WolfLabs/eventos/src/view/evento-cadastro/index.js",[],{"ruleId":"55","replacedBy":"56"},{"ruleId":"57","replacedBy":"58"},{"ruleId":"59","severity":1,"message":"60","line":7,"column":10,"nodeType":"61","messageId":"62","endLine":7,"endColumn":18},"no-native-reassign",["63"],"no-negated-in-lhs",["64"],"no-unused-vars","'Redirect' is defined but never used.","Identifier","unusedVar","no-global-assign","no-unsafe-negation"]